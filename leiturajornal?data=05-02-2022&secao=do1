		var triggerShowEvent = ['mouseenter', 'MSPointerDown', 'touchstart'];

		if (A.UA.ios) {
			triggerShowEvent = ['touchstart'];
		}

		Liferay.Data.LFR_PORTAL_TOOLTIP = new A.TooltipDelegate(
			{
				constrain: true,
				opacity: 1,
				trigger: '.lfr-portal-tooltip',
