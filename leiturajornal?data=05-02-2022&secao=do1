			<div class=" portlet-content-container" >
				


	<div class="portlet-body">



	
		
			
				
					



















































	

				

				
					
						


	

		
































	
		
			
				
					
						<div class="text-right user-tool-asset-addon-entries">
							
						</div>

						















<div class="clearfix journal-content-article" data-analytics-asset-id="278244339" data-analytics-asset-title="REPORTAR ERRO" data-analytics-asset-type="web-content">
	

	  <script>
    $(document).ready(function(){
      var checkboxes = $('.checkboxes');
      checkboxes.change(function(){
        if($('.checkboxes:checked').length>0) {
            checkboxes.removeAttr('required');
        } else {
            checkboxes.attr('required', 'required');
        }
      });

      $("#erro5").change(function(){
        $(".outros").toggleClass("d-none");
        $("#Outros").attr('required', function(_, attr){ return !attr});
      });

      var blacklisted_words = `FGTS; INSS; PIS; PASEP;`;
      var blacklisted_words_arr = blacklisted_words.split(";");
      $('form#report-error').submit(function(e) {
        var erro_arr = [];
        $("input[name=erro]:checked").each(function() {
          var erro = $(this).val();
          erro_arr.push(erro);
        });
        $(this).append('<input type="hidden" name="Error" value="'+ erro_arr +'" /> ');

        var valid = true;
        // Get all the forms elements and their values in one step
        var descricao = $("#Descrição").val(); 
        for(var i=0; i < blacklisted_words_arr.length-1; i++){
          if(descricao.toLowerCase().includes(blacklisted_words_arr[i].toLowerCase().trim())){
            $("#send").val("false");
            valid = false;
            $("body").append('<div id="alertContainer" class="alertContainer alert-notifications-fixed"><div class="alert alert-report alert-danger alert-dismissible fade show" role="alert">Este canal é exclusivamente para manifestações relativas a falhas e inconsistências na publicação.<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div></div>');
            setTimeout(() => {$("#alertContainer").hide()}, 10000);
            return valid;
          }
        }
        const piii = new Piii({
          filters: [
            ...Object.values(piiiFilters)
          ]
        });
        if (piii.has(descricao)){
          $("#send").val("false");
          valid = false;
          $("body").append('<div id="alertContainer" class="alertContainer alert-notifications-fixed"><div class="alert alert-report alert-danger alert-dismissible fade show" role="alert">Este canal é exclusivamente para manifestações relativas a falhas e inconsistências na publicação.<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div></div>');
          setTimeout(() => {$("#alertContainer").hide()}, 10000);
          return valid;
        }
        $("#send").val("true");
        return valid; 
      });
      if($.cookie("reportSuccess") === "true"){
        $("body").append('<div id="alertContainer" class="alert-notifications alert-notifications-fixed"><div class="alert alert-report alert-info alert-dismissible fade show" role="alert">Erro reportado com sucesso.<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div></div>');
        setTimeout(() => {$("#alertContainer").hide()}, 5000);
        $.cookie("reportSuccess", "");
      }
    });
  </script>

</div>



						
					
				
			
		
	



